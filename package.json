{
  "name": "@ignatisd/nearest-neighbor",
  "version": "2.0.0",
  "description": "Helps with the nearest neighbor problem",
  "main": "nearestNeighbor.js",
  "scripts": {
    "commit": "cz",
    "test": "node test.js",
    "docs": "node nearestNeighbor.js && node node_modules/jsdoc-to-markdown/bin/cli.js --template README.hbs nearestNeighbor.js > README.md",
    "preview-docs": "node node_modules/jsdoc-to-markdown/bin/cli.js --json nearestNeighbor.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ignatisd/nearest-neighbor.git"
  },
  "author": "Ignatios Drakoulas",
  "license": "MIT",
  "devDependencies": {
    "assert": "^2.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "jsdoc-to-markdown": "^6.0.1",
    "semantic-release": "^17.3.8"
  },
  "publishConfig": {
    "access": "public",
    "branches": [
      "master"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
